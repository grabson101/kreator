var app = angular.module('myApp', []);
app.controller('customersCtrl', function($scope, $http) {
getTable();
function getTable()
{
    $http.get("/php/app/getTable.php")
    .then(function (response) {$scope.hours = response.data;});
};


$scope.updateTable = function (dataID,iloscGodzin)
{
  $http.get("php/app/updateTable.php?dataID="+dataID+"&iloscGodzin="+iloscGodzin).success(function(data){
      getTable();
    });
  };

});
