var app = angular.module('myApp', []);
app.controller('customersCtrl', function($scope, $http) {
getTable();
$scope.heh = "kurdebalans";
function getTable()
{

    $http.get(Routing.generate('getTable'))
    .then(function (response) {$scope.hours = response.data;});
};


$scope.updateTable = function (e, ilosc_godzin)
{
  ilosc_godzin-=8;
  if(ilosc_godzin<0)
    ilosc_godzin=16;
  alert(e.target.innerHTML = ilosc_godzin);
  };

});
